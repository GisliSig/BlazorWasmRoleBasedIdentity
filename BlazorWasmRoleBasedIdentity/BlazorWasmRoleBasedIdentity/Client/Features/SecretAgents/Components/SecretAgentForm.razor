<h3>@Title</h3>
<EditForm Model="Agent" OnValidSubmit="Save">
    <div class="form-group">
        <label>Name</label>
        <InputText class="form-control" @bind-Value="Agent.Name"></InputText>
    </div>
    <div class="form-group">
        <label>Alias</label>
        <InputText class="form-control" @bind-Value="Agent.Alias"></InputText>
    </div>
    <button class="btn btn-primary" type="submit">Save</button>
</EditForm>
